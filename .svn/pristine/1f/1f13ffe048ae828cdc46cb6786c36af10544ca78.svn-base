#ft_course {
    .content {
        .container {
            .tab_head {
                ul {
                    li {
                        float: left;
                        height: 40px;
                        margin-left: 10px;
                        line-height: 40px;
                        cursor: pointer;
                        text-align: center;
                        padding: 0 10px;
                    }
                }
                .tab_nav {
                    ul {
                        zoom: 1;
                        font-size: 14px;
                        @media (max-width: 768px) {
                            display: none;
                        }
                    }
                    .style {
                        border-top: 1px solid #D0D0D0;
                    }
                }
            }
            .course_center {
                .list {
                    display: block;
                    width: 100%;
                    margin-top: 15px;
                    height: 110px;
                    border-bottom: 1px solid #ddd;
                    .list_left {
                        padding: 0;
                        background-color: #CFCDED;
                        position: relative;
                        overflow: hidden;
                        border-radius: 5px;
                        height: 90px;
                        text-align: center;
                        .season {
                            position: absolute;
                            top: -5px;
                            left: -5px;
                            display: block;
                            font-size: 18px;
                            width: 30px;
                            height: 30px;
                            border-radius: 50%;
                            text-align: center;
                            line-height: 30px;
                            background-color: #fff;
                        }
                        .glyphicon {
                            font-size: 40px;
                            margin-top: 10px;
                        }
                        h3 {
                            text-align: center;
                            font-size: 18px;
                            margin-top: 10px;
                            text-shadow: 0px 0px 6px #fff;
                        }
                        &.chem {
                            background-color: #f3dab2;
                        }
                        &.phy {
                            background-color: #b2e8f4;
                        }
                        &.math {
                            background-color: #cecded;
                        }
                    }
                    .list_right {
                        padding: 0;
                        padding-left: 10px;
                        h2 {
                            margin-top: 0;
                            font-size: 14px;
                            font-weight: 600;
                            text-align: left;
                        }
                        ul {
                            li {
                                float: left;
                                float: left;
                                margin-right: 5px;
                                line-height: 20px;
                                font-size: 12px;
                                i {
                                    display: inline-block;
                                    width: 2px;
                                    height: 2px;
                                    line-height: 1;
                                    border-radius: 50%;
                                    background-color: #000;
                                    vertical-align: middle;
                                    margin-right: 5px;
                                }
                            }
                        }
                        p {
                            font-size: 12px;
                            margin: 5px;
                        }
                        .price {
                            font-size: 12px;
                        }
                        .status {
                            float: right;
                            margin-right: 15px;
                        }
                        .status.full {
                            color: #bebebe;
                        }
                        .status.danger {
                            color: #EF0602;
                        }
                        .status.hot {
                            color: #F7941D;
                        }
                    }
                }
            }
            #course_end {
                text-align: center;
                margin-bottom: 10px;
                margin-top: 10px;
                cursor: pointer;
            }
        }
    }
    @media (min-width: 769px) {
        .content {
            margin-top: 100px;
            .container {
                .tab_head {
                    .yx {
                        position: fixed;
                        width: 100%;
                        height: 50px;
                        line-height: 50px;
                        top: 50px;
                        left: 0;
                        background-color: #AFAFAF;
                        z-index: 99;
                        .container {
                            height: 50px;
                            span {
                                margin-left: 15px;
                                text-align: center;
                                display: block;
                                width: 80px;
                                height: 100%;
                                float: left;
                            }
                            ul {
                                margin-left: 105px;
                                height: 50px;
                                line-height: 50px;
                                li.current {
                                    width: 100px;
                                    height: 28px;
                                    margin-top: 11px;
                                    line-height: 28px;
                                    background-color: #29C2E1;
                                    color: white;
                                    margin-left: 15px;
                                    padding: 0 20px;
                                }
                            }
                        }
                    }
                    .tab_nav {
                        .list {
                            padding: 5px 15px;
                            span {
                                text-align: center;
                                display: block;
                                width: 80px;
                                height: 100%;
                                float: left;
                                height: 40px;
                                line-height: 40px;
                            }
                            ul {
                                margin-left: 90px;
                                li {
                                    width: 100px;
                                    text-align: center;
                                    height: 28px;
                                    margin-top: 6px;
                                    margin-bottom: 10px;
                                    line-height: 28px;
                                    background-color: #fff;
                                    color: black;
                                    margin-left: 15px;
                                    padding: 0;
                                    // border: 1px solid #DEDEDE;
                                }
                                li.current {
                                    background-color: #29C2E1;
                                    color: white;
                                }
                            }
                        }
                    }
                }
                .course_center {
                    margin-top: 30px;
                    margin-bottom: 15px;
                    .list {
                        height: 150px;
                        .list_left {
                            height: 120px;
                            background-color: #B2E8F4;
                            h3 {
                                margin-top: 18px;
                            }
                            .season {
                                width: 50px;
                                height: 50px;
                                top: -10px;
                                left: -10px;
                                padding-top: 10px;
                                font-size: 18px;
                            }
                            .glyphicon {
                                font-size: 40px;
                                margin-top: 30px;
                            }
                        }
                        .list_right {
                            padding-left: 50px;
                            h2 {
                                font-size: 26px;
                                margin-bottom: 10px;
                                text-overflow: ellipsis;
                                white-space: nowrap;
                                text-overflow: ellipsis;
                                overflow: hidden;
                            }
                            ul {
                                margin-top: 5px;
                                text-overflow: ellipsis;
                                white-space: nowrap;
                                overflow: hidden;
                                text-overflow: ellipsis;
                                li {
                                    display: inline-block;
                                    float: none;
                                    margin-bottom: 5px;
                                }
                            }
                            p {
                                margin: 10px 0;
                            }
                            .pullright {
                                .status {
                                    float: none;
                                    margin-left: 180px;
                                    font-size: 24px;
                                }
                            }
                        }
                        .col-sm-3 {
                            height: 120px;
                            display: flex;
                            flex-direction: column;
                            justify-content: center;
                            .num {
                                display: block;
                                width: 75px;
                                height: 75px;
                                border-radius: 50%;
                                border: 1px solid #D5D5D5;
                                margin: 0 auto;
                                text-align: center;
                                padding-top: 5px;
                                font-size: 12px;
                                span {
                                    font-family: ftFont;
                                    font-size: 40px;
                                }
                            }
                            .btn_ft {
                                display: block;
                                width: 80px;
                                height: 30px;
                                margin: 0 auto;
                                margin-top: 10px;
                                text-align: center;
                                line-height: 30px;
                                font-size: 16px;
                            }
                        }
                    }
                }
            }
        }
    }
    @media (max-width: 768px) {
        .content {
            margin-top: 40px;
            .container {
                .tab_head {
                    position: fixed;
                    top: 40px;
                    left: 3%;
                    width: 94%;
                    background-color: #fff;
                    z-index: 99;
                    padding: 20px 0px 10px;
                }
                .tab_nav {
                    height: 34px;
                    border: 2px solid #29c2e1;
                    border-radius: 4px;
                    &>div {
                        border: none;
                    }
                    .list {
                        padding: 0;
                        border: none;
                        border-right: 2px solid #29c2e1;
                        z-index: 99;
                        ul {
                            display: none;
                            position: fixed;
                            top: 110px;
                            left: 3%;
                            width: 94%;
                            border: 0px solid #29c2e1;
                            background-color: #29c2e1;
                            color: white;
                            border-radius: 5px;
                            overflow: hidden;
                            li {
                                margin-left: 0;
                            }
                        }
                        span {
                            display: inline-block;
                            width: 100%;
                            height: 100%;
                            line-height: 32px;
                            text-align: center;
                            color: #29c2e1;
                            font-size: 16px;
                        }
                    }
                    .area,
                    .sort,
                    .subject {
                        ul {
                            li {
                                width: 100%;
                                float: none;
                                border-bottom: 1px solid #B1E4F2;
                                &:nth-last-child(1) {
                                    border-bottom: none;
                                }
                            }
                        }
                    }
                    .grad {
                        li {
                            width: 33.3%;
                            border-bottom: 1px solid #B1E4F2;
                            border-right: 1px solid #B1E4F2;
                            &:nth-child(3n+0) {
                                border-right: none;
                            }
                            &:nth-child(n+10) {
                                border-bottom: none;
                            }
                        }
                    }
                    .sort {
                        border: none;
                    }
                }
                .course_center {
                    margin-top: 65px;
                }
            }
        }
    }
}
