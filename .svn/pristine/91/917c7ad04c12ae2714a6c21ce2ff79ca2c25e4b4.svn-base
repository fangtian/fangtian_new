body,
ul,
dl,
dt,
dd,
ol,
p,
li {
  margin: 0;
  padding: 0;
}
a {
  color: #555;
  text-decoration: none;
}
a:hover {
  text-decoration: none;
  color: inherit;
  background-color: inherit;
}
img {
  border: none;
}
ol,
ul,
li {
  list-style: none;
}
table {
  border-collapse: collapse;
}
html {
  overflow-y: scroll;
  height: 100%;
}
body {
  height: 100%;
  background: #fff;
  color: black;
  font-size: 14px;
  padding-right: 0!important;
  font-family: "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", tahoma, arial, simsun, "宋体";
}
@font-face {
  font-family: ftFont;
