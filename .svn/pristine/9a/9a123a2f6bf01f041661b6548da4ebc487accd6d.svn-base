    #user_center {
        // padding: 20px 0;
        background-color: #F2F1F1;
        .user_content {
            select {
                display: block;
                margin: 10px auto;
                height: 35px;
                line-height: 35px;
                border-radius: 5px;
                border-color: #54C3E1;
                background-color: white;
                color: #54C3E1;
                font-size: 16px;
                letter-spacing: 2px;
                padding: 0 5px;
            }
            .exam {
                .user_exam {
                    input {
                        margin: 20px 0;
                    }
                    .answer {
                        margin: 20px 0;
                        position: relative;
                        .btn_ft {
                            display: block;
                            width: 80px;
                            height: 30px;
                            line-height: 30px;
                            text-align: center;
                            margin: 0 auto;
                            position: absolute;
                            top: -15px;
                            left: 50%;
                            transform: translate(-50%)
                        }
                        .answer_content {
                            background-color: #fff;
                            padding: 20px 5px;
                            ins {
                                color: #E00E12;
                            }
                        }
                    }
                    .exam_num {
                        width: 100%;
                        display: flex;
                        justify-content: space-around;
                        padding: 5px 0;
                        span {
                            width: 30px;
                            height: 30px;
                            border-radius: 50%;
                            line-height: 30px;
                            border: 1px solid #000;
                            text-align: center;
                            cursor: pointer;
                            &.current {
                                color: #0099FF;
                                border-color: #0099FF;
                            }
                        }
                    }
                    .tollbar {
                        width: 100%;
                        margin: 15px auto;
                        display: flex;
                        justify-content: space-around;
                        padding: 5px 0;
                        .btn_ft {
                            background-color: #ED9324;
                            width: 80px;
                            border-style: none;
                            font-size: 16px;
                            letter-spacing: 3px;
                            &:active,
                            &:hover,
                            &:focus {
                                background-color: #ED9324;
                                border: none;
                                outline: none;
                                box-shadow: none;
                            }
                        }
                    }
                    .content {
                        .options {
                            li {
                                // margin: 5px 0;
                                padding: 10px 5px;
                                cursor: pointer;
                                font-size: 16px;
                                letter-spacing: 1px;
                                border-bottom: 1px solid #ccc;
                                &:nth-child(1) {
                                    border-top: 1px solid #ccc;
                                }
                                &:hover,
                                &.checked {
                                    color: #EFA346;
                                    span {
                                        border-color: #EFA346;
                                    }
                                }
                                &.right {
                                    color: green;
                                    span {
                                        border-color: green;
                                        color: white;
                                        background-color: green;
                                    }
                                }
                                &.wrong {
                                    color: red;
                                    span {
                                        border-color: red;
                                        color: white;
                                        background-color: red;
                                    }
                                }
                                span {
                                    display: inline-block;
                                    margin-right: 5px;
                                    width: 20px;
                                    height: 20px;
                                    border-radius: 50%;
                                    border: 1px solid #ccc;
                                    text-align: center;
                                    line-height: 20px;
                                    font-size: 14px;
                                    padding-left: 2px;
                                }
                            }
                        }
                    }
                }
                .user_score {
                    min-height: 120px;
                    margin: 15px 0;
                    .result {
                        position: relative;
                        min-height: 60px;
                        .wrap,
                        .circle,
                        .percent {
                            position: absolute;
                            width: 60px;
                            height: 60px;
                            border-radius: 50%;
                        }
                        .wrap {
                            top: 0;
                            left: 20px;
                            background-color: #fff;
                        }
                        .circle {
                            box-sizing: border-box;
                            border: 10px solid #fff;
                            clip: rect(0, 60px, 60px, 30px);
                        }
                        .clip-auto {
                            clip: rect(auto, auto, auto, auto);
                        }
                        .percent {
                            box-sizing: border-box;
                            top: -10px;
                            left: -10px;
                        }
                        .left {
                            transition: transform ease;
                            border: 10px solid #54C3F9;
                            clip: rect(0, 30px, 60px, 0);
                        }
                        .right {
                            border: 10px solid #54C3F9;
                            clip: rect(0, 60px, 60px, 30px);
                        }
                        .wth0 {
                            width: 0;
                        }
                        .num {
                            position: absolute;
                            box-sizing: border-box;
                            width: 40px;
                            height: 40px;
                            line-height: 40px;
                            text-align: center;
                            font-size: 12px;
                            left: 10px;
                            top: 10px;
                            border-radius: 50%;
                            background-color: #fff;
                            z-index: 1;
                        }
                    }
                    .user_result {
                        margin-top: -7px;
                        margin-left: 80px;
                        display: flex;
                        justify-content: space-around;
                        text-align: center;
                        font-size: 16px;
                        span {
                            display: block;
                            margin-top: 0px;
                            font-size: 24px;
                            color: #ED9324;
                        }
                    }
                }
                .user_analyse {}
            }

        }
        
